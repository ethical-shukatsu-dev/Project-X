# Supabase configuration
NEXT_PUBLIC_SUPABASE_URL=your_supabase_url
NEXT_PUBLIC_SUPABASE_ANON_KEY=your_supabase_anon_key
# Get this from Supabase dashboard > Project Settings > API > service_role key
NEXT_PUBLIC_SUPABASE_SERVICE_ROLE_KEY=your_supabase_service_role_key

# Environment control
# Set to 'production' to force production mode regardless of NODE_ENV
# NEXT_PUBLIC_APP_ENV=production

# OpenAI API configuration
OPENAI_API_KEY=your_openai_api_key 
# Admin API configuration
API_SECRET_KEY=your_admin_api_key
# BrandFetch configuration
BRANDFETCH_CLIENT_ID=your_brandfetch_client_id
# Pexels API configuration
PEXELS_API_KEY=your_pexels_api_key

# Add Google OAuth Client ID for Google Sign-In
GOOGLE_CLIENT_ID=your-google-client-id.apps.googleusercontent.com
# Google AI API Configuration
GOOGLE_API_KEY=your-google-api-key

# Facebook Pixel Configuration
NEXT_PUBLIC_FB_PIXEL_ID=your_facebook_pixel_id

# Microsoft Clarity Configuration
NEXT_PUBLIC_CLARITY_PROJECT_ID=your_clarity_project_id
# Set to 'true' to enable Clarity in development
# NEXT_PUBLIC_ENABLE_CLARITY=true
